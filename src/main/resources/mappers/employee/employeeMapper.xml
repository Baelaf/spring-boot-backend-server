<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.backend.backendserver.dao.EmployeeDao">
    <select id="selectEmployeeList" resultType="EmployeeDto">
        SELECT EMP_NO
             , EMP_ID
             , EMP_PWD
             , EMP_NM
             , IDENTITY_FRONT
             , IDENTITY_BACK
        FROM TB_EMP_MSTR
    </select>
</mapper>